<h1>Listing masters</h1>

<table>
  <tr>
    <th>Home</th>
    <th>Homescore</th>
    <th>Away</th>
    <th>Awayscore</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @masters.each do |master| %>
  <tr>
    <td><%= master.home %></td>
    <td><%= master.homescore %></td>
    <td><%= master.away %></td>
    <td><%= master.awayscore %></td>
    <td><%= link_to 'Show', master %></td>
    <td><%= link_to 'Edit', edit_master_path(master) %></td>
    <td><%= link_to 'Destroy', master, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Master', new_master_path %>
